{
  "expo": {
    "name": "routes42",
    "slug": "routes42",
    "version": "1.0.0",
    "scheme": "routes42",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-maps",
        {
          "googleMapsApiKey": "AIzaSyAlm3Es35ecfHTp4-gb7MjAfoqEcWuKXX0"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.domingos-franco.x42Routes",
      "config": {
        "googleMapsApiKey": "AIzaSyAlm3Es35ecfHTp4-gb7MjAfoqEcWuKXX0"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "package": "com.domingos_franco.x42Routes",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAlm3Es35ecfHTp4-gb7MjAfoqEcWuKXX0"
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "b38f6974-5f2c-46ec-894c-1973e241ee00"
      }
    }
  }
}
